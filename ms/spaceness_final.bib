  interhash = {bf3963ffd0edea4c5099e2aef106af48},
  intrahash = {beeb3ccd0b0fe954295a730834e17f68},
  issn = {0737-4038},
  journal = {Molecular Biology and Evolution},
  keywords = {Wrights_neighborhood_size isolation_by_distance landscape_genetics methods},
  month = {10},
