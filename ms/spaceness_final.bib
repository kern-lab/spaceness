	doi = {10.1534/genetics.118.301594},
	publisher = {Genetics},
	abstract = {The genetics and evolution of complex traits, including quantitative traits and disease, have been hotly debated ever since Darwin. A century ago, a paper from R.A. Fisher reconciled Mendelian and biometrical genetics in a landmark contribution that is now accepted as the main foundation stone of the field of quantitative genetics. Here, we give our perspective on Fisher{\textquoteright}s 1918 paper in the context of how and why it is relevant in today{\textquoteright}s genome era. We mostly focus on human trait variation, in part because Fisher did so too, but the conclusions are general and extend to other natural populations, and to populations undergoing artificial selection.},
	issn = {0016-6731},
	URL = {http://www.genetics.org/content/211/4/1125},
	eprint = {http://www.genetics.org/content/211/4/1125.full.pdf},
	journal = {Genetics}
}


