@article{Kerster1964,
 ISSN = {00143820, 15585646},
 URL = {http://www.jstor.org/stable/2406355},
 abstract = {Mark and recapture data on over 3,000 rusty lizards, Sceloporus olivaceus Smith, followed for 5 consecutive years on a 4-hectare study tract, were examined to deterine the neighborhood size component of spatial gene flow restriction. (1) The genetically important dispersal movements of the lizards are the females' nesting excursions (29 meters) and one to three changes of home range by juveniles (78 meters from hatching to adulthood). (2) The neighborhood areas as calculated for both males (9.3 hectares) and females (11.0 hectares) are approximately the same (average 10 hectares). (3) The genetically effective number of lizards on a neighborhood is 80 females and 190-425 males. Neighborhood size is 225-270. (4) Generation span in the rusty lizard population is about 20.6 months. (5) The breeding structure of the rusty lizard provides for faster evolution than panmixia. (6) Populations with overlapping generations are less likely to experience drastic long-term effective size reduction due to occasional poor production of offspring than populations with discrete generations.},
 author = {Harold W. Kerster},
 journal = {Evolution},
 number = {3},
 pages = {445--457},
 publisher = {[Society for the Study of Evolution, Wiley]},
 title = {Neighborhood Size in the Rusty Lizard, \textit{{Sceloporus} olivaceus}},
